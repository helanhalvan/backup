<welcome>
  <!-- waypoints - define before the agents! -->
  <waypoint id="w1" x="0" y="60" r="10" />
  <waypoint id="w2" x="160" y="60" r="10" />

  <!-- agents -->
  <agent x="0" y="60" n="200" dx="40" dy="60">
    <addwaypoint id="w1" />
    <addwaypoint id="w2" />
  </agent>

  <agent x="160" y="60" n="200" dx="50" dy="30">
    <addwaypoint id="w2" />
    <addwaypoint id="w1" />
  </agent>

  <waypoint id="w3" x="80" y="10" r="10" />
  <waypoint id="w4" x="80" y="120" r="10" />

  <agent x="70" y="20" n="200" dx="40" dy="60">
    <addwaypoint id="w3" />
    <addwaypoint id="w4" />
  </agent>

  <agent x="70" y="120" n="200" dx="40" dy="60">
    <addwaypoint id="w4" />
    <addwaypoint id="w3" />
  </agent>

</welcome>
