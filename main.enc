import Ped_util.Global_funs
import Ped_util.Regions
class Main
  def main(args : [String]) : unit
    let
      agents = parse_file(args(1))
      ui = match (args(2)).to_int() with
             case Just(1) =>
               true
             end
             case _ =>
               false
             end
           end
      boxes = if |args| < 4 then
                5
              else
                match (args(3)).to_int() with
                  case Just(x) =>
                    x
                  end
                  case _ =>
                    5
                  end
                end
              end
    in
     regions(agents,ui,boxes)
    end
  end
end
